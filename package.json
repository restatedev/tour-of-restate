{
  "name": "tour-of-restate-typescript",
  "version": "0.0.1",
  "description": "Tour of Restate's key features with the Typescript SDK",
  "type": "commonjs",
  "scripts": {
    "proto": "cd proto && npx buf mod update && npx buf generate .",
    "build": "tsc --noEmitOnError",
    "ticket_service": "ts-node-dev --watch ./src --respawn --transpile-only ./src/ticket_service.ts",
    "user_session_service": "ts-node-dev --watch ./src --respawn --transpile-only ./src/user_session_service.ts",
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\""
  },
  "author": "<your name here>",
  "license": "ISC",
  "dependencies": {
    "@restatedev/restate-sdk": "^1.0.17",
    "protobufjs": "^7.2.2",
    "ts-proto": "^1.140.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "prettier": "^2.8.8",
    "eslint": "^8.40.0",
    "@bufbuild/buf": "1.15.0"
  }
}
